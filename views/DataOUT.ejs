<%- include("Components/Header.ejs") %>
<style>
  himanshu{
    font-size: 250px;
  }
</style>
  <body>
    <himanshu>
      The Data in Himanshu
    </himanshu>
    <div class="container w-10">
      <div class="row">
        <div class="col-6">
          <% imgData['profile'].forEach(img=> { %>
            <img src="/Uploads/<%= img.originalname %>" class="img-fluid" alt="<%= img.originalname %>">
            <% }) %>
        </div>
        <div class="col-6 py-5">
          <h1>
            <%= textData.name %>
          </h1>
          <p>
            <%= textData.email %>
          </p>
          <div class="row">
          <% imgData['uploads'].forEach(img => { %>
            <div class="col-4">
              <img class="img-fluid" src="/Uploads/<%= img.originalname %> " alt="">
            </div>
            <% }) %>
          </div>
        </div>
      </div>

    </div>

  </body>

  </html>