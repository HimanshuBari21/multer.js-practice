<%- include("Components/Header.ejs", {title: "Profile" }) %>

  <body>
    <div class="container w-10">
      <div class="row">
        <div class="col-6">
          <% imgData['profile'].forEach(img=> { %>
            <img src="/uploads/<%= img.originalname %>" class="img-fluid" alt="<%= img.originalname %>">
            <% }) %>
        </div>
        <div class="col-6 py-5">

          <h1>
            <%= textData.name %>
          </h1>
          <p>
            <%= textData.email %>
          </p>

        </div>
      </div>
      <div class="row">

      </div>
    </div>

  </body>

  </html>